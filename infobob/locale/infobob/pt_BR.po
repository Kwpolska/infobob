#: infobob/irc.py:245
#, python-format
msgid "no triggers in %s."
msgstr "%s não possui triggers."

#: infobob/irc.py:328
#, python-format
msgid ""
"in the future, please use a less awful pastebin (e.g. paste.pocoo.org) "
"instead of %s."
msgstr ""
"por favor, da próxima vez use um site menos terrível que %s (por exemplo "
"paste.pocoo.org)."

#: infobob/irc.py:346
#, python-format
msgid "%(url)s (repasted for %(user)s)"
msgstr "%(url)s (recolado para %(user)s)"

#: infobob/irc.py:357 infobob/irc.py:383 infobob/irc.py:395
#, python-format
msgid "Error: %r"
msgstr "Erro: %r"

#: infobob/irc.py:406
#, python-format
msgid "(... %(nlines)d lines, entire response in %(url)s ...)"
msgstr "(... %(nlines)d linhas, resposta completa em %(url)s ...)"

#: infobob/irc.py:421
msgid "Done."
msgstr "Concluído."

#: infobob/irc.py:423
msgid "Database not loaded."
msgstr "Banco de dados não carregado."

#: infobob/irc.py:433
msgid "Database unlocked."
msgstr "Banco de dados destrancado."

#: infobob/irc.py:435 infobob/irc.py:444
msgid "Database was unlocked."
msgstr "Banco de dados estava destrancado."

#: infobob/irc.py:442
msgid "Database locked."
msgstr "Banco de dados trancado."

#: infobob/irc.py:452
#, python-format
msgid "%d days"
msgstr "%d dias"

#: infobob/irc.py:456
#, python-format
msgid "%d hours"
msgstr "%d horas"

#: infobob/irc.py:458
#, python-format
msgid "%d minutes"
msgstr "%d minutos"

#: infobob/irc.py:460
#, python-format
msgid "%d seconds"
msgstr "%d segundos"

#: infobob/irc.py:466
#, python-format
msgid "%(group)s and %(last)s"
msgstr "%(group)s e %(last)s"

#: infobob/irc.py:470
#, python-format
msgid ""
"I have been online for %(time_online)s. In that time, I've processed %"
"(wordcount)d characters and spliced %(chaincount)d chains. Currently, I "
"reference %(dblen)d chains with %(begin)d beginnings (%(actions)d actions)."
msgstr ""
"Eu estive online por %(time_online)s. Neste tempo, eu processei %(wordcount)"
"d caracteres e gerei %(chaincount)d cadeias. Faço referência neste momento à "
"%(dblen)d cadeias com %(begin)d inícios (%(actions)d ações)."

#: infobob/irc.py:489
msgid "shakes the psychic black sphere."
msgstr "olha para sua bola de cristal."

#: infobob/irc.py:495
#, python-format
msgid "It says: \"%s\""
msgstr "Ela diz: \"%s\""

#: infobob/irc.py:502
msgid "line too short."
msgstr "linha muito curta."

#: infobob/irc.py:511
msgid "inf"
msgstr "inf"

#: infobob/irc.py:513
#, python-format
msgid ""
"%(chance)0.6f%% chance (1 in %(inverse)s) across %(prob_len)d probabilities; "
"%(avg)0.6f%% average, standard deviation %(deviation)0.6f%%, %(min_prob)0.6f%"
"% low, %(max_prob)0.6f%% high."
msgstr ""
"%(chance)0.6f%% de chance (uma em %(inverse)s) em %(prob_len)d "
"probabilidades; média %(avg)0.6f%%, desvio padrão %(deviation)0.6f%%, mínimo "
"%(min_prob)0.6f%%, máximo %(max_prob)0.6f%%."

#: infobob/irc.py:528
msgid "Okay!"
msgstr "Certo!"

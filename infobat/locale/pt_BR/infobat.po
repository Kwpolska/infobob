#: infobat/irc.py:470
#, python-format
msgid ""
"I have been online for %(time_online)s. In that time, I've processed %"
"(wordcount)d characters and spliced %(chaincount)d chains. Currently, I "
"reference %(dblen)d chains with %(begin)d beginnings (%(actions)d actions)."
msgstr ""
"Eu estive online por %(time_online)s. Neste tempo, eu processei "
"%(wordcount)d caracteres e gerei %(chaincount)d cadeias. Faço referência "
"neste momento à %(dblen)d cadeias com %(begin)d inícios (%(actions)d ações)."

#: irc.py:465
msgid "shakes the psychic black sphere."
msgstr "olha para sua bola de cristal."

#: irc.py:471
msgid "It says: \"%s\""
msgstr "Ela diz: \"%s\""

#: infobat/irc.py:492
msgid "line too short."
msgstr "linha muito curta."

#: infobat/irc.py:505
msgid "inf"
msgstr "inf"

#: infobat/irc.py:513
#, python-format
msgid ""
"%(chance)0.6f%% chance (1 in %(inverse)s) across %(prob_len)d probabilities; "
"%(avg)0.6f%% average, standard deviation %(deviation)0.6f%%, %(min_prob)0.6f%"
"% low, %(max_prob)0.6f%% high."
msgstr ""
"%(chance)0.6f%% de chance (uma em %(inverse)s) em %(prob_len)d "
"probabilidades; média %(avg)0.6f%%, desvio padrão %(deviation)0.6f%%, "
"mínimo %(min_prob)0.6f%%, máximo %(max_prob)0.6f%%."

#: irc.py:519
msgid "Okay!"
msgstr "Certo!"

